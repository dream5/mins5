<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE web-app PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN" "http://java.sun.com/dtd/web-app_2_3.dtd">
<web-app>
	<context-param>
		<param-name>
			javax.servlet.jsp.jstl.fmt.localizationContext
		</param-name>
		<param-value>ApplicationResources</param-value>
	</context-param>
	<context-param>
		<param-name>contextConfigLocation</param-name>
		<param-value>
			/WEB-INF/classes/applicationContext-plugins.xml
		</param-value>
	</context-param>

	<filter>
		<filter-name>EncodingFilter</filter-name>
		<filter-class>
			com.asiainfo.frame.web.filter.EncodingFilter
		</filter-class>
		<init-param>
			<param-name>encoding</param-name>
			<param-value>gb2312</param-value>
		</init-param>
		<init-param>
			<param-name>ignore</param-name>
			<param-value>true</param-value>
		</init-param>
	</filter>
	<filter-mapping>
		<filter-name>EncodingFilter</filter-name>
		<url-pattern>*.go</url-pattern>
	</filter-mapping>
	<filter>
		<filter-name>exportFilter</filter-name>
		<filter-class>
			org.displaytag.render.ResponseOverrideFilter
		</filter-class>
	</filter>

	<filter>
		<filter-name>actionFilter</filter-name>
		<filter-class>com.asiainfo.crm.web.ActionFilter</filter-class>
		<init-param>
			<param-name>obviablePath</param-name>
			<param-value>
				/userRegAction.go,/userLoginAction.go,/index.jsp,/pwdRestoreAction.go,/productAction.go,/userSpProductAction.go,/manager/managerLoginAction.go,/userQueryAction.go,/acceptQueryAction.go,/searchAcceptAction.go,/productSearchAction.go,/foreignbusi/,/foreignultimate/,/terminal/
			</param-value>
		</init-param>
	</filter>

	<filter>
		<filter-name>kpicollector</filter-name>
		<filter-class>
			com.asiainfo.frame.web.filter.KPICollector
		</filter-class>
	</filter>


	<filter-mapping>
		<filter-name>EncodingFilter</filter-name>
		<url-pattern>*.jsp</url-pattern>
	</filter-mapping>
	<filter-mapping>
		<filter-name>EncodingFilter</filter-name>
		<url-pattern>/dwr/*</url-pattern>
	</filter-mapping>
	<filter-mapping>
		<filter-name>actionFilter</filter-name>
		<url-pattern>*.go</url-pattern>
	</filter-mapping>
	<filter-mapping>
		<filter-name>actionFilter</filter-name>
		<url-pattern>/index.jsp</url-pattern>
	</filter-mapping>
	<filter-mapping>
		<filter-name>kpicollector</filter-name>
		<url-pattern>*.go</url-pattern>
	</filter-mapping>
	<filter-mapping>
		<filter-name>exportFilter</filter-name>
		<url-pattern>*.go</url-pattern>
	</filter-mapping>
	<listener>
		<listener-class>
			org.springframework.web.util.IntrospectorCleanupListener
		</listener-class>
	</listener>
	<listener>
		<listener-class>
			<!--  org.springframework.web.context.ContextLoaderListener -->
			com.asiainfo.frame.common.ContextLoaderListener
		</listener-class>
	</listener>
	<listener>
		<listener-class>
			com.asiainfo.crm.web.WebSessionListener
		</listener-class>
	</listener>
	<listener>
		<listener-class>
			com.asiainfo.crm.task.AppTask
		</listener-class>
	</listener>
	<servlet>
		<servlet-name>action</servlet-name>
		<servlet-class>
			org.apache.struts.action.ActionServlet
		</servlet-class>
		<init-param>
			<param-name>config</param-name>
			<param-value>
				/WEB-INF/struts-config-common.xml,
				/WEB-INF/struts-config-userdoor.xml,
				/WEB-INF/struts-config-manager.xml,
				/WEB-INF/struts-config-resource.xml,
				/WEB-INF/struts-config-group.xml,
				/WEB-INF/struts-config-score.xml,
				/WEB-INF/struts-config-club.xml,
				/WEB-INF/struts-config-pc.xml,
				/WEB-INF/struts-config-pm.xml,
				/WEB-INF/struts-config-term.xml,
				/WEB-INF/struts-config-channel.xml,
				/WEB-INF/struts-config-nb.xml,
				/WEB-INF/struts-config-foreignbusi.xml,
				/WEB-INF/struts-config-foreignultimate.xml,
				/WEB-INF/struts-config-orgForeign.xml,
				/WEB-INF/struts-config-blacklist.xml,
				/WEB-INF/struts-config-resic.xml,
				/WEB-INF/struts-config-3gaccounts.xml,
				/WEB-INF/struts-config-3gauditpackage.xml,
				/WEB-INF/struts-config-rescard.xml,
				/WEB-INF/struts-config-ftns.xml
			</param-value>
		</init-param>
		<init-param>
			<param-name>debug</param-name>
			<param-value>3</param-value>
		</init-param>
		<init-param>
			<param-name>detail</param-name>
			<param-value>3</param-value>
		</init-param>
		<load-on-startup>0</load-on-startup>
	</servlet>

	<servlet>
		<servlet-name>dwr-invoker</servlet-name>
		<servlet-class>uk.ltd.getahead.dwr.DWRServlet</servlet-class>
		<init-param>
			<param-name>debug</param-name>
			<param-value>false</param-value>
		</init-param>
		<init-param>
			<param-name>logLevel</param-name>
			<param-value>ERROR</param-value>
		</init-param>
	</servlet>


	<servlet>
		<servlet-name>AxisServlet</servlet-name>
		<display-name>Apache-Axis Servlet</display-name>
		<servlet-class>
			org.apache.axis.transport.http.AxisServlet
		</servlet-class>
	</servlet>

	<servlet>
		<servlet-name>randomSevlet</servlet-name>
		<servlet-class>
			com.asiainfo.crm.web.RandomServlet
		</servlet-class>
	</servlet>

	<servlet>
		<servlet-name>RandomImage</servlet-name>
		<servlet-class>
			com.asiainfo.frame.util.RandomImage
		</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>ServletPortal</servlet-name>
		<servlet-class>
			com.asiainfo.crm.service.impl.resic.interfaces.servlet.ServletPortal
		</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>BatchCardServlet</servlet-name>
		<servlet-class>
			com.asiainfo.crm.service.impl.resic.interfaces.servlet.BatchCardServlet
		</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>BatchCardServlet</servlet-name>
		<url-pattern>/servlet/BatchCardServlet</url-pattern>
	</servlet-mapping>



	<servlet-mapping>
		<servlet-name>randomSevlet</servlet-name>
		<url-pattern>*.random</url-pattern>
	</servlet-mapping>

	<servlet-mapping>
		<servlet-name>AxisServlet</servlet-name>
		<url-pattern>/servlet/AxisServlet</url-pattern>
	</servlet-mapping>

	<servlet-mapping>
		<servlet-name>AxisServlet</servlet-name>
		<url-pattern>*.jws</url-pattern>
	</servlet-mapping>

	<servlet-mapping>
		<servlet-name>AxisServlet</servlet-name>
		<url-pattern>/services/*</url-pattern>
	</servlet-mapping>

	<servlet-mapping>
		<servlet-name>dwr-invoker</servlet-name>
		<url-pattern>/dwr/*</url-pattern>
	</servlet-mapping>

	<servlet-mapping>
		<servlet-name>RandomImage</servlet-name>
		<url-pattern>/randomImage</url-pattern>
	</servlet-mapping>


	<servlet-mapping>
		<servlet-name>action</servlet-name>
		<url-pattern>*.go</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>ServletPortal</servlet-name>
		<url-pattern>/servlet/ServletPortal</url-pattern>
	</servlet-mapping>
	<session-config>
		<session-timeout>60</session-timeout>
	</session-config>


	<welcome-file-list>
		<welcome-file>index.jsp</welcome-file>
	</welcome-file-list>

	<taglib>
		<taglib-uri>/WEB-INF/struts-template.tld</taglib-uri>
		<taglib-location>/WEB-INF/struts-template.tld</taglib-location>
	</taglib>
	<taglib>
		<taglib-uri>/WEB-INF/struts-tiles.tld</taglib-uri>
		<taglib-location>/WEB-INF/struts-tiles.tld</taglib-location>
	</taglib>
	<taglib>
		<taglib-uri>/WEB-INF/struts-nested.tld</taglib-uri>
		<taglib-location>/WEB-INF/struts-nested.tld</taglib-location>
	</taglib>
	<taglib>
		<taglib-uri>/WEB-INF/struts-bean.tld</taglib-uri>
		<taglib-location>/WEB-INF/struts-bean.tld</taglib-location>
	</taglib>
	<taglib>
		<taglib-uri>/WEB-INF/struts-html.tld</taglib-uri>
		<taglib-location>/WEB-INF/struts-html.tld</taglib-location>
	</taglib>
	<taglib>
		<taglib-uri>/WEB-INF/struts-logic.tld</taglib-uri>
		<taglib-location>/WEB-INF/struts-logic.tld</taglib-location>
	</taglib>

	<taglib>
		<taglib-uri>/WEB-INF/asiainfo-core.tld</taglib-uri>
		<taglib-location>/WEB-INF/asiainfo-core.tld</taglib-location>
	</taglib>
	<taglib>
		<taglib-uri>/WEB-INF/asiainfo-crm.tld</taglib-uri>
		<taglib-location>/WEB-INF/asiainfo-crm.tld</taglib-location>
	</taglib>


</web-app>